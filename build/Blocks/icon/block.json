{
  "$schema": "https://schemas.wp.org/trunk/block.json",
  "apiVersion": 2,
  "name": "yard-gutenberg/icon",
  "version": "0.1.0",
  "title": "Icon",
  "category": "layout",
  "icon": "smiley",
  "description": "Voeg een Font Awesome icoon toe.",
  "supports": {
    "align": true,
    "ariaLabel": true,
    "color": true,
    "spacing": {
      "margin": true,
      "padding": true
    }
  },
  "attributes": {
    "icon": {
      "type": "string",
      "default": "fal fa-envelope"
    },
    "iconSize": {
      "type": "string",
      "default": "18px"
    },
    "altText": {
      "type": "string",
      "default": ""
    }
  },
  "textdomain": "yard-gutenberg",
  "editorScript": "file:./index.js",
  "editorStyle": "file:./index.css",
  "style": "file:./style-index.css"
}